/* pages/myLogs/myLogs.wxss */
.header {
  background: #17b963;
  width: 100%;
  height: 150px;
}
.num{
  color: white;
  font-size: 60px;
  font-family: DINNextLTPro-Condensed;
}
.animate-number{
  color: white;
  font-size: 16px;
  margin-left: 30px;
  width: 82%;
  display: inline-block;
}
.service{
  width: 10px;
  height: 14px;
  display: inline-block;
}
.service-image{
  width: 100%;
  height: 100%;
}
.body{
 
}
page{
  height: 100%;
   background: #F5F4F9;
}
.body-title{
  color:#999999;
  font-size: 14px;
  margin-top: 10px;
  margin-left: 15px;
}
.devices-list{
  width: 100%;
  margin-top: 10px;
  height: auto;
}
.device{
  height: 125px;
  background: #FFFFFF;
  display: flex;
align-items: center;
}
.device-img{
  display: inline-block;
  width: 95px;
  height: 95px;
  /* padding-left: 15px; */
}
.device-info{
  display: inline-block;
  height: 95px;
  justify-content: center; 
align-items: center;
padding-left: 15px;
padding-top: 8px;
width: 65%;
}
.device-body{
  width: 100%;
  height: 95px;
  display: flex;
  /* justify-content: center;  */
  padding-left:5%;
  align-items: center;
}
.device-cnt{
  font-size: 16px;
  line-height: 27px;
  font-weight: 600;
  width: 100%;
}
.location{
  font-size: 12px;
color: #666666;
letter-spacing: 0;
line-height: 27px;
font-weight: 500;
padding-left: 30px;
float: right;
}
.change-time{
  font-family: PingFang-SC-Regular;
font-size: 13px;
color: #666666;
letter-spacing: 0;
line-height: 27px;
}
.price{
  font-family: PingFang-SC-Medium;
font-size: 21px;
color: #17B963;
letter-spacing: 0;
line-height: 27px;
}
doller{
  font-family: PingFang-SC-Medium;
font-size: 14px;
color: #17B963;
letter-spacing: 0;
line-height: 27px;
}
.warp button {
    position: absolute;
    border-radius:0px 0px 0px 0px;
    color: transparent;
    background:transparent no-repeat center center;
    background-size: cover;
    height: 100%;
    width: 100%;
    top: 0;
    left: 0;
    z-index: 10;
}