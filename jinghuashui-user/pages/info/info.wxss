.purifier{
    display: block;
}
.detail-address{
    position:relative;
    padding:12px 15px;
    display:flex;
    background:#fff;
    align-items:center;
    font-size:14px;
    overflow:hidden;
    color:#495060;
}
.login{
    display: flex;
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 100 !important;
    background: blue;
    opacity:0;
}
.line{
    width: 95%;
    height: 12px;
    margin: 0 auto;
    background: black;
}
.container{
  width: 100%;
  background: #EFEEF4;
  height: 100%;
}
i-card{
  width: 100%;
  position: absolute;
  top: 0
}
.i-body{
  border-radius:12px;
  overflow: hidden;
  background: white;
}
page{
  height: 100%;
}
navigator{
  border: none;
  border-radius: 0;
  color: #17B963;
  background: white;
  box-shadow: none;
  height: 21px;
  width: 40%;
  font-size: 15px;
  display: inline-block;
}
